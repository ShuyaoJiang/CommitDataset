[+++ b/spring-boot-cli/src/main/scripts/spring, +#!/usr/bin/env bash, +# For Cygwin, ensure paths are in UNIX format before anything is touched., +if $cygwin ; then, +    [ -n "$JAVA_HOME" ] && JAVA_HOME=`cygpath --unix "$JAVA_HOME"`, +fi, +, +, +# Attempt to find JAVA_HOME if not already set, +# Sanity check that we have java, +# Attempt to find SPRING_HOME if not already set, +# Resolve links: $0 may be a link, +PRG="$0", +# Need this for relative symlinks., +while [ -h "$PRG" ] ; do, +    ls=`ls -ld "$PRG"`, +    link=`expr "$ls" : '.*-> \(.*\)$'`, +    if expr "$link" : '/.*' > /dev/null; then, +        PRG="$link", +        PRG=`dirname "$PRG"`"/$link", +    fi, +done, +SAVED="`pwd`", +cd "`dirname \"$PRG\"`/../" >&-, +SPRING_HOME="`pwd -P`", +cd "$SAVED" >&-, +, +for f in "${SPRING_HOME}"/classes "${SPRING_HOME}"/*.jar "${SPRING_HOME}"/lib/*.jar; do, +    SPRING_HOME=`cygpath --path --mixed "$SPRING_HOME"`, +"${JAVA_HOME}/bin/java ${JAVA_OPTS}" -cp "$CLASSPATH" org.springframework.boot.cli.SpringCli $*]